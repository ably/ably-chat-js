{
  "language": "en_US",
  "ignorePaths": ["node_modules/**", "**/dist/**"],
  "useGitignore": true,
  "maxNumberOfProblems": 100,
  "words": [
    "realtime",
    "inband",
    "typers",
    "msgpack",
    "dequal",
    "Unregisters",
    "Suspendable",
    "Failable",
    "livestreams",
    "livestream",
    "clonedeep",
    "timeserial"
  ],
  // This will ignore anything that's formatted like a lexicographically ordered timeserial
  "ignoreRegExpList": ["/\\d{14}-\\d{3}@.*/"],
  "flagWords": ["cancelled"]
}
